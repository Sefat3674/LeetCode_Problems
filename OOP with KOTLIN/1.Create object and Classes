class Bankaccount(var bankowner: String, var balance: Double){
    
    fun deposit(amount: Double){
        balance += amount
        println("Deposited $amount,new balance: $balance")
        
    }
    fun withdraw(amount: Double){
        if(balance>=amount){
            balance -=amount
            println("withdraw $amount, new balance: $balance ")
        }
        else{
            println("Insufficient balance")
        }
        	
    }
    
}

fun main() {
    var myaccount=Bankaccount("Sefat",1000.0)
    myaccount.withdraw(3000.0)
    myaccount.deposit(300.0)
}

